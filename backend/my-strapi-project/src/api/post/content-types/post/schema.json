{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "author": {
      "type": "component",
      "repeatable": false,
      "component": "author.author"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "excerpt": {
      "type": "string"
    },
    "tags": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "options": [
        "Food",
        "IRCTC",
        "Ipsator",
        ""
      ]
    },
    "slug": {
      "type": "string"
    },
    "featuredImage": {
      "type": "component",
      "repeatable": true,
      "component": "featured-image.featured-image"
    },
    "date": {
      "type": "string"
    },
    "categories": {
      "type": "json"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tagNames": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "tag"
    }
  }
}
